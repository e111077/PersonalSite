{
  "name": "personal-site",
  "version": "0.0.1",
  "description": "Elliott's Personal Website",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && polymer build && cp -a build/fallback/lib/. build/default/fallback && rm -rf build/fallback && npm run clean-export",
    "dev": "tsc --watch & polymer serve --npm -o",
    "serveprod": "firebase serve -o 0.0.0.0",
    "deploy": "npm run build && firebase deploy",
    "clean-export": "sed -i '.bak' 's/export{.*$//' build/default/fallback/components/elliott-main-page.js && rm build/default/fallback/components/elliott-main-page.js.bak"
  },
  "author": "me@elliottmarquez.dev",
  "license": "MIT",
  "devDependencies": {
    "@types/redux": "^3.6.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "lit-element": "^2.1.0",
    "pwa-helpers": "^0.9.1"
  }
}
